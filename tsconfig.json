{
  "compilerOptions": {
    "outDir": "./assets/javascript",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "removeComments": false,
    "allowSyntheticDefaultImports": false,
    "jsx": "react",
    "include":[
      "./src/**/*"
    ]
  }
}
